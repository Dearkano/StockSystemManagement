{
  "compilerOptions": {
    "noImplicitAny": false,
    "removeComments": false,
    "sourceMap": false,
    "target": "es5",
    "lib": ["dom", "es5", "es6"],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "experimentalDecorators": true,
  },
  "exclude": [
    "node_modules",
    "wwwroot",
  ]
}
